{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "wpdsfr/alert",
  "version": "0.1.0",
  "title": "Alert",
  "category": "theme",
  "description": "Les alertes permettent d’attirer l’attention de l’utilisateur sur une information sans interrompre sa tâche en cours.",
  "attributes": {
    "title": {
      "type": "string",
      "default": ""
    },
    "level": {
      "type": "integer",
      "default": 3
    },
    "levelOptions": {
      "type": "array"
    },
    "content": {
      "type": "string"
    },
    "type": {
      "type": "string"
    },
    "size": {
      "type": "string",
      "default": "md"
    }
  },
  "supports": {
    "className": false,
    "customClassName": false
  },
  "textdomain": "wpdsfr",
  "editorScript": "file:./index.js",
  "style": "file:./style-index.css",
  "render": "file:./render.php"
}