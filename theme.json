{
	"$schema": "https://schemas.wp.org/trunk/theme.json",
	"version": 2,
	"settings": {
		"appearanceTools": false,
		"color": {
			"text": false,
			"background": false,
			"defaultGradients": false,
			"customDuotone": false,
			"link": false,
			"custom": false,
			"customGradient": false,
			"palette": []
		},
		"typography": {
			"fontFamilies": [
				{
					"fontFace": [
						{
							"fontFamily": "Marianne",
							"fontStretch": "normal",
							"fontStyle": "normal",
							"fontWeight": "300",
							"src": ["file:./assets/fonts/marianne/Marianne-Light.woff2"]
						},
						{
							"fontFamily": "Marianne",
							"fontStretch": "normal",
							"fontStyle": "italic",
							"fontWeight": "300",
							"src": ["file:./assets/fonts/marianne/Marianne-Light_Italic.woff2"]
						},
						{
							"fontFamily": "Marianne",
							"fontStretch": "normal",
							"fontStyle": "normal",
							"fontWeight": "400",
							"src": ["file:./assets/fonts/marianne/Marianne-Regular.woff2"]
						},
						{
							"fontFamily": "Marianne",
							"fontStretch": "normal",
							"fontStyle": "italic",
							"fontWeight": "400",
							"src": ["file:./assets/fonts/marianne/Marianne-Regular_Italic.woff2"]
						},
						{
							"fontFamily": "Marianne",
							"fontStretch": "normal",
							"fontStyle": "normal",
							"fontWeight": "700",
							"src": ["file:./assets/fonts/marianne/Marianne-Bold.woff2"]
						},
						{
							"fontFamily": "Marianne",
							"fontStretch": "normal",
							"fontStyle": "italic",
							"fontWeight": "700",
							"src": ["file:./assets/fonts/marianne/Marianne-Bold_Italic.woff2"]
						}
					],
					"fontFamily": "\"Marianne\", arial, sans-serif",
					"name": "Marianne",
					"slug": "default"
				},
				{
					"fontFace": [
						{
							"fontFamily": "Spectral",
							"fontStretch": "normal",
							"fontStyle": "normal",
							"fontWeight": "400",
							"src": ["file:./assets/fonts/spectral/Spectral-Regular.woff2"]
						},
						{
							"fontFamily": "Spectral",
							"fontStretch": "normal",
							"fontStyle": "normal",
							"fontWeight": "800",
							"src": ["file:./assets/fonts/spectral/Spectral-Heavy.woff2"]
						}
					],
					"fontFamily": "\"Spectral\", georgia, serif",
					"name": "Spectral",
					"slug": "secondary"
				}
			]
		}
	},
	"styles": {
		"typography": {
			"fontFamily": "var(--wp--preset--font-family--default)"
		}
	}
}