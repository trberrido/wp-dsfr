{
	"$schema": "https://schemas.wp.org/trunk/theme.json",
	"version": 2,
	"title": "Light",
	"settings": {
		"custom": {
			"color": {
				"neutral": {
					"000": "#DDDDDD"
				}
			}
		},
		"appearanceTools": false,
		"useRootPaddingAwareAlignments": true,
		"color": {
			"text": false,
			"background": false,
			"defaultGradients": false,
			"customDuotone": false,
			"custom": false,
			"customGradient": false,
			"palette": [
				{
					"slug": "primary",
					"color": "#000091",
					"name": "Primary"
				},
				{
					"slug": "secondary",
					"color": "#E1000F",
					"name": "Secondary"
				},
				{
					"slug": "base",
					"color": "#FFFFFF",
					"name": "Background"
				},
				{
					"slug": "contrast",
					"color": "#161616",
					"name": "Text"
				},
				{
					"slug": "background-action-hover",
					"color": "#1212FF",
					"name": "Background hover"
				},
				{
					"slug": "background-alt",
					"color": "#F6F6F6",
					"name": "Background alternatif"
				},
				{
					"slug": "neutral",
					"color": "#DDDDDD",
					"name": "Neutre"
				}
			]
		},
		"typography": {
			"fontSizes": [
				{
					"slug": "text-xxs",
					"size": "1.25rem",
					"name": "Title XXS"
				}
			],
			"fontFamilies": [
				{
					"fontFace": [
						{
							"fontFamily": "Marianne",
							"fontStretch": "normal",
							"fontStyle": "normal",
							"fontWeight": "300",
							"src": ["file:./assets/fonts/marianne/Marianne-Light.woff2"]
						},
						{
							"fontFamily": "Marianne",
							"fontStretch": "normal",
							"fontStyle": "italic",
							"fontWeight": "300",
							"src": ["file:./assets/fonts/marianne/Marianne-Light_Italic.woff2"]
						},
						{
							"fontFamily": "Marianne",
							"fontStretch": "normal",
							"fontStyle": "normal",
							"fontWeight": "400",
							"src": ["file:./assets/fonts/marianne/Marianne-Regular.woff2"]
						},
						{
							"fontFamily": "Marianne",
							"fontStretch": "normal",
							"fontStyle": "italic",
							"fontWeight": "400",
							"src": ["file:./assets/fonts/marianne/Marianne-Regular_Italic.woff2"]
						},
						{
							"fontFamily": "Marianne",
							"fontStretch": "normal",
							"fontStyle": "normal",
							"fontWeight": "700",
							"src": ["file:./assets/fonts/marianne/Marianne-Bold.woff2"]
						},
						{
							"fontFamily": "Marianne",
							"fontStretch": "normal",
							"fontStyle": "italic",
							"fontWeight": "700",
							"src": ["file:./assets/fonts/marianne/Marianne-Bold_Italic.woff2"]
						}
					],
					"fontFamily": "\"Marianne\", arial, sans-serif",
					"name": "Marianne",
					"slug": "default"
				},
				{
					"fontFace": [
						{
							"fontFamily": "Spectral",
							"fontStretch": "normal",
							"fontStyle": "normal",
							"fontWeight": "400",
							"src": ["file:./assets/fonts/spectral/Spectral-Regular.woff2"]
						},
						{
							"fontFamily": "Spectral",
							"fontStretch": "normal",
							"fontStyle": "normal",
							"fontWeight": "800",
							"src": ["file:./assets/fonts/spectral/Spectral-Heavy.woff2"]
						}
					],
					"fontFamily": "\"Spectral\", georgia, serif",
					"name": "Spectral",
					"slug": "secondary"
				}
			]
		},
		"layout": {
			"contentSize": "75rem",
			"wideSize": "77rem"
		},
		"spacing": {
			"units": ["rem"],
			"blockGap": true
		}
	},
	"styles": {
		"blocks": {
			"core/button": {
				"css": "&:hover {background-color: var(--wp--preset--color--background-action-hover)}",
				"border": {
					"radius": "0"
				},
				"color": {
					"background": "var(--wp--preset--color--primary)",
					"text": "white"
				},
				"variations": {
					"outline": {
						"border": {
							"color": "var(--wp--preset--color--primary)"
						},
						"color": {
							"text": "var(--wp--preset--color--primary)"
						}
					}
				}
			},
			"core/paragraph": {
				"elements": {
					"link": {
						":hover": {
							"css": "opacity: .5"
						}
					}
				}
			},
			"core/heading": {
				"color": {
					"text": "var(--wp--preset--color--text-title)"
				}
			},
			"core/navigation-link": {
				"elements": {
					"link": {
						"spacing": {
							"padding": {
								"top": "1.5rem",
								"right": "1rem",
								"bottom": "1.5rem",
								"left": "1rem"
							}
						},
						":hover": {
							"color": {
								"background": "var(--wp--preset--color--neutral)"
							}
						}
					}
				}
			},
			"core/site-title": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--text-xxs)",
					"fontWeight": "700"
				},
				"color": {
					"text": "var(--wp--preset--color--text-default)"
				}
			}
		},
		"color": {
			"background": "var(--wp--preset--color--background)",
			"text": "var(--wp--preset--color--text-default)"
		},
		"typography": {
			"fontFamily": "var(--wp--preset--font-family--default)"
		},
		"spacing": {
			"padding": {
				"top": "1.5rem",
				"right": "1.5rem",
				"bottom": "1.5rem",
				"left": "1.5rem"
			}
		},
		"elements": {
			"heading": {
				"color": {
					"text": "var(--wp--preset--color--contrast)"
				}
			},
			"link": {
				"color": {
					"text": "var(--wp--preset--color--primary)"
				}
			}
		}
	}
}